	<parameter name="fechaInicial" class="java.lang.String"/>
	<parameter name="fechaFinal" class="java.lang.String"/>
	<query language="sql"><![CDATA[SELECT 
    c.id AS id, 
    c.nombre AS nombre, 
    c.apellido_paterno AS apellido_paterno, 
    c.apellido_materno AS apellido_materno,
    c.ocupacion AS ocupacion, 
    e.nombre AS estado, 
    d.colonia AS colonia, 
    c.telefono1 AS telefono1,
    c.telefono2 AS telefono2, 
    c.fecha_registro AS fecha_registro 
FROM clientes c
LEFT JOIN domicilios d ON c.id = d.cliente_id
LEFT JOIN estados e ON d.estado_id = e.id
 where fecha_registro between $P{fechaInicial} and $P{fechaFinal}]]></query>
